<link
  rel="preload"
  href="/assets/fonts/garpwarp/Garpwarp-Regular.otf"
  as="font"
  type="font/otf"
  crossorigin
/>

<link
  rel="preload"
  href="/assets/fonts/garpwarp/Garpwarp-Warped.otf"
  as="font"
  type="font/otf"
  crossorigin
/>

<!-- preloads in page data -->
{%- if preloads -%}
  <!-- prettier-ignore -->
  <link rel="preload"
			href="{{ preloads.href }}"
			as="{{ preloads.as }}"
			{% if preloads.type %}type={{ preloads.type }}{% endif %}{% if preloads.imagesrcset %}imagesrcset="{{ preloads.imagesrcset }}"{% endif %}
			{% if preloads.crossorigin %}crossorigin{% endif %} />
{% endif %}
